#include <iostream>
#include <vector>
#include <unordered_map>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<long long> arr(n);
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    long long target;
    cin >> target;

    unordered_map<long long, int> mp; // removed the stray '4'

    for (int i = 0; i < n; i++) {
        long long need = target - arr[i];
        if (mp.find(need) != mp.end()) {
            cout << "(" << mp[need] << "," << i << ")" << endl;
            return 0;
        }
        mp[arr[i]] = i;
    }

    cout << "No such potions found." << endl;
    return 0;
}
